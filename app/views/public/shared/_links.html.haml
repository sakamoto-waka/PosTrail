- if controller_name != 'sessions'
  .devise-link
    = link_to "既にユーザーの方はこちら", new_session_path(resource_name), class: "devise-link-a"
  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to "Sign up", new_registration_path(resource_name), class: "devise-link"
  %br/
